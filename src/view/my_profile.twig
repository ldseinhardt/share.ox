{% extends "layout/logged.twig" %}

{% block page %}

  <section class="panel panel-primary">
    <div class="panel-heading">
      <h1 class="panel-title" style="font-size: 16pt">
        <img src="{{ path('file', {file: 'images/icon-share.png'}) }}">Minha biblioteca
      </h1>
    </div>
    <div class="panel-body">
      <div>{{ user.first_name }} {{ user.last_name }}</div>
      <div><img src="{{ user.picture }}" class="img-circle"></div>
      <div>Curso: {{ user.couse_id }} - {{user.institution_id }}</div>
      <div>Cidade: {{user.city_id}}</div>
    </div>
  </section>

{% endblock %}
